{
  "name": "localia-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "codegen": "apollo client:codegen --target typescript --localSchemaFile schema.gql --outputFlat --includes \"{pages,src}/**\" --excludes \"src/generated/**\" src/generated",
    "db:init": "yarn prisma init",
    "db:generate": "yarn prisma generate",
    "db:migrate:new": "yarn prisma migrate save --experimental",
    "db:migrate:up": "yarn prisma migrate up --experimental",
    "db:migrate:down": "yarn prisma migrate down --experimental",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "@prisma/client": "~2.11.0",
    "@reach/combobox": "^0.15.1",
    "apollo-server-micro": "^2.19.0",
    "class-validator": "^0.13.1",
    "cloudinary": "^1.25.1",
    "cloudinary-react": "^1.7.0",
    "firebase": "^8.6.1",
    "firebase-admin": "^9.8.0",
    "geolib": "^3.3.1",
    "graphql": "^15.4.0",
    "js-cookie": "^2.2.1",
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-firebaseui": "^5.0.2",
    "react-hook-form": "^7.6.3",
    "react-map-gl": "^5.2.10",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "use-debounce": "^6.0.1",
    "use-google-maps-script": "^0.1.2",
    "use-places-autocomplete": "^1.9.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@prisma/cli": "~2.11.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.2.9",
    "@types/js-cookie": "^2.2.6",
    "@types/react": "^17.0.5",
    "apollo": "^2.31.1",
    "autoprefixer": "^9",
    "babel-loader": "^8.2.2",
    "babel-plugin-parameter-decorator": "^1.0.16",
    "babel-plugin-transform-typescript-metadata": "^0.3.1",
    "postcss": "^7",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "typescript": "^4.2.4"
  }
